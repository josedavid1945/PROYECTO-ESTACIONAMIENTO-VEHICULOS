# Configuraci√≥n para base de datos LOCAL (Docker)
# Copia este archivo como .env en websocket-server/ para usar BD local

# WebSocket Server
WS_PORT=8081
WS_PATH=/ws
AUTO_UPDATE_INTERVAL=5

# PostgreSQL LOCAL
DB_HOST=localhost
DB_PORT=5432
DB_USER=admin
DB_PASSWORD=admin123
DB_NAME=estacionamiento
DB_SSLMODE=disable

# Connection pool
DB_MAX_OPEN_CONNS=25
DB_MAX_IDLE_CONNS=5
DB_CONN_MAX_LIFETIME=300
